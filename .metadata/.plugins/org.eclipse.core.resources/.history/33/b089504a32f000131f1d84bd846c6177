
public class Booleans {
	public static void main(String[] args) {
		boolean x = true;
		boolean y = false;
		
		if(x) {
			System.out.println(x);
		}
		
		if(y == false) {
			System.out.println(y);
		}
		
		int int1 = 2;
		int int2 = 6;
		
		if(int1 >= 2) {
			System.out.println(int1 + " is greater than 2");
		}
	}
}
